# Azure Service Principal Credentials
# Create a Service Principal: az ad sp create-for-rbac --name "dokploy-sp" --role Contributor --scopes /subscriptions/<SUBSCRIPTION_ID>
subscription_id = "your-subscription-id-here"
client_id       = "your-client-id-here"
client_secret   = "your-client-secret-here"
tenant_id       = "your-tenant-id-here"

# Dokploy Admin Password (used for initial setup)
dokploy_admin_password = "YourSecurePassword123!"

# Enable/Disable Dokploy automation after VM creation
enable_dokploy_setup = true
